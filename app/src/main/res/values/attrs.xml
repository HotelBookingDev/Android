<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PersonalItemView">
        <attr name="leftIcon" format="reference" />
        <attr name="contextText" format="string" />
        <attr name="rightText" format="string" />
    </declare-styleable>

    <declare-styleable name="HotelTitleBar">
        <attr name="leftBtnText" format="string" />
        <attr name="rightBtnText" format="string" />
        <attr name="leftBtnIcon" format="reference" />
        <attr name="rightBtnIcon" format="reference" />
        <attr name="titleText" format="string" />
        <attr name="isShowUnderLine" format="boolean" />
    </declare-styleable>
    <declare-styleable name="ToggleButton">
        <attr name="tbwidth" format="dimension" />
        <attr name="offBorderColor" format="reference|color" />
        <attr name="offColor" format="reference|color" />
        <attr name="onColor" format="reference|color" />
        <attr name="spotColor" format="reference|color" />
        <attr name="animate" format="reference|boolean" />
    </declare-styleable>

    <declare-styleable name="CircleIndicator">
        <attr name="indicator_radio" format="dimension" />
        <attr name="indicator_margin" format="dimension" />
        <attr name="indicator_background" format="color|integer" />
        <attr name="indicator_selected_background" format="color|integer" />
    </declare-styleable>

    <declare-styleable name="CustomRadioButton">
        <attr name="text_color" format="color|integer"/>
        <attr name="select_text_color" format="color|integer"/>
    </declare-styleable>


    <declare-styleable name="HideTitle">
        <attr name="mid_text" format="string"/>
        <attr name="left_img" format="reference"/>
        <attr name="right_img1" format="reference"/>
        <attr name="right_img2" format="reference"/>
        <attr name="add_status_view" format="boolean"/>
        <attr name="hide_background" format="color"/>
        <attr name="title_size" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="ListText">
        <attr name="title_text" format="string"/>
        <attr name="content_text" format="string"/>

    </declare-styleable>

    <declare-styleable name="PullToZoomView">
        <attr name="headerView" format="reference" />
        <attr name="contentView" format="reference" />
        <attr name="zoomView" format="reference" />
        <attr name="isHeaderParallax" format="boolean" />
    </declare-styleable>

    <declare-styleable name="PriceLayout">
        <attr name="now_price" format="string"/>
        <attr name="old_price" format="string"/>
        <attr name="left_price_img" format="reference"/>
    </declare-styleable>


    <declare-styleable name="HotelSearchItem">
        <attr name="text" format="string"/>
        <attr name="hint" format="string"/>
        <attr name="close_src" format="reference"/>

    </declare-styleable>

    <declare-styleable name="HotelSearchTitle">
        <attr name="hotel_title_content_text" format="string"/>
        <attr name="tag_text" format="string"/>
        <attr name="des_text" format="string"/>

    </declare-styleable>

    <declare-styleable name="CustomSearchItem">
        <attr name="custom_item_img" format="reference"/>
        <attr name="custom_item_text1" format="string"/>
        <attr name="custom_item_text2" format="string"/>
        <attr name="custom_item_style" format="integer"/>
    </declare-styleable>

    <declare-styleable name="PullScrollView">
        <attr name="pull_header_layout" format="reference"/>
        <attr name="pull_content_layout" format="reference"/>
        <attr name="pull_is_eff" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="DayPickerView">
        <attr name="colorCurrentDay" format="color"/>
        <attr name="colorHideDay" format="color"/>
        <attr name="colorSelectedDayBackground" format="color"/>
        <attr name="colorSelectedDayText" format="color"/>
        <attr name="colorPreviousDay" format="color"/>
        <attr name="colorNormalDay" format="color" />
        <attr name="colorMonthName" format="color" />
        <attr name="colorDayName" format="color" />
        <attr name="textSizeDay" format="dimension"/>
        <attr name="textSizeMonth" format="dimension" />
        <attr name="textSizeDayName" format="dimension" />
        <attr name="headerMonthHeight" format="dimension" />
        <attr name="selectedDayRadius" format="dimension" />
        <attr name="calendarHeight" format="dimension" />
        <attr name="enablePreviousDay" format="boolean" />
        <attr name="currentDaySelected" format="boolean" />
        <attr name="drawRoundRect" format="boolean" />
        <attr name="firstMonth" format="enum">
            <enum name="january" value="0" />
            <enum name="february" value="1" />
            <enum name="march" value="2" />
            <enum name="april" value="3" />
            <enum name="may" value="4" />
            <enum name="june" value="5" />
            <enum name="july" value="6" />
            <enum name="august" value="7" />
            <enum name="september" value="8" />
            <enum name="october" value="9" />
            <enum name="november" value="10" />
            <enum name="december" value="11" />
        </attr>
        <attr name="lastMonth" format="enum">
            <enum name="january" value="0" />
            <enum name="february" value="1" />
            <enum name="march" value="2" />
            <enum name="april" value="3" />
            <enum name="may" value="4" />
            <enum name="june" value="5" />
            <enum name="july" value="6" />
            <enum name="august" value="7" />
            <enum name="september" value="8" />
            <enum name="october" value="9" />
            <enum name="november" value="10" />
            <enum name="december" value="11" />
        </attr>
    </declare-styleable>
</resources>